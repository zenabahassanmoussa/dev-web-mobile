#include <stdio.h>

// Fonction pour calculer la surface d'un carré
double surfaceCarre(double cote) {
    return cote * cote;
}

int main() {
    double cote;
    printf("Entrez la longueur du côté du carré : ");
    scanf("%lf", &cote);
    
    double surface = surfaceCarre(cote);
    printf("La surface du carré est : %.2f\n", surface);
    
    return 0;
}